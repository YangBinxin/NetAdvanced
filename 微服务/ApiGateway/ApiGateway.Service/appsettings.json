{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  //动态接口文档
  "Swagger": {
    "Name": "ApiGateway.Service",
    "Title": "网关服务",
    "Version": "v1"
  },
  //支持Swagger的API 名称
  "Apis": {
    "SwaggerNames": "SwaggerAPI1;SwaggerAPI2"
  },
  // IdentityService 身份认证
  "IdentityService": {
    "Uri": "http://192.168.109.65:23006", //认证服务IP
    "DefaultScheme": "IdentityBearer",  //标识名称
    "UseHttps": false, //启用https
    "ApiName": "default-api", //认证服务名称
    "ApiSecret": "def2edf7-5d42-4edc-a84a-30136c340e13"  //secret值
  }
}
